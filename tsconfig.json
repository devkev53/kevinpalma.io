{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/js": [
        "src/js/*"
      ],
      "@/store": [
        "src/store/*"
      ],
      "@/i18n": [
        "src/i18n/*"
      ],
    },
    // "jsx": "react-jsx",
    // "jsxImportSource": "react"
  }
}
